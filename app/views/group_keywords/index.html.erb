<p id="notice"><%= notice %></p>

<h1>Group Keywords</h1>

<table>
  <thead>
    <tr>
      <th>Groupid</th>
      <th>Userid</th>
      <th>Keyword</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_keywords.each do |group_keyword| %>
      <tr>
        <td><%= group_keyword.groupid %></td>
        <td><%= group_keyword.userid %></td>
        <td><%= group_keyword.keyword %></td>
        <td><%= link_to 'Show', group_keyword %></td>
        <td><%= link_to 'Edit', edit_group_keyword_path(group_keyword) %></td>
        <td><%= link_to 'Destroy', group_keyword, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


